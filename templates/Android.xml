<templateSet group="Android">
  <template name="note" value="/**&#10; * $desc$&#10; * @author JQJM_CaoJiaqi&#10; * @date $data$.&#10; */" shortcut="ENTER" description="activaty 类注释" toReformat="false" toShortenFQNames="true">
    <variable name="desc" expression="descendantClassesEnum(String)" defaultValue="" alwaysStopAt="true" />
    <variable name="data" expression="date()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="butt" value="compile 'com.jakewharton:butterknife:8.5.1'&#10;annotationProcessor 'com.jakewharton:butterknife-compiler:8.5.1'" description="butterknife自动加入" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="GROOVY" value="true" />
    </context>
  </template>
</templateSet>